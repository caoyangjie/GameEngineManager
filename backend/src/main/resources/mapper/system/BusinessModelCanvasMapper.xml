<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gameengine.system.mapper.BusinessModelCanvasMapper">
    
    <resultMap type="com.gameengine.system.domain.BusinessModelCanvas" id="BusinessModelCanvasResult">
        <id     property="canvasId"              column="canvas_id"               />
        <result property="title"                 column="title"                   />
        <result property="keyPartners"           column="key_partners"            />
        <result property="keyActivities"         column="key_activities"          />
        <result property="keyResources"          column="key_resources"           />
        <result property="valuePropositions"     column="value_propositions"      />
        <result property="customerRelationships" column="customer_relationships"  />
        <result property="channels"              column="channels"                />
        <result property="customerSegments"      column="customer_segments"       />
        <result property="costStructure"         column="cost_structure"          />
        <result property="revenueStreams"        column="revenue_streams"         />
        <result property="status"                column="status"                  />
        <result property="createBy"              column="create_by"               />
        <result property="createTime"            column="create_time"             />
        <result property="updateBy"              column="update_by"               />
        <result property="updateTime"            column="update_time"             />
        <result property="remark"                column="remark"                  />
    </resultMap>
    
    <sql id="selectBusinessModelCanvasVo">
        select canvas_id, title, key_partners, key_activities, key_resources, value_propositions, 
               customer_relationships, channels, customer_segments, cost_structure, revenue_streams, 
               status, create_by, create_time, update_by, update_time, remark
        from business_model_canvas
    </sql>
    
</mapper>
